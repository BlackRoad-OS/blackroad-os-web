{
  "name": "blackroad-web",
  "version": "0.1.0",
  "private": true,
  "description": "BlackRoad - Composable agent orchestration platform",
  "scripts": {
    "dev": "next dev",
    "build": "next build && rm -rf .out && mv out .out",
    "start": "serve -s .out -l ${PORT:-3000}",
    "lint": "next lint",
    "test": "vitest run --passWithNoTests && playwright test",
    "postbuild": "tsx scripts/postbuild.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check ."
  },
  "dependencies": {
    "next": "14.2.3",
    "next-plausible": "^3.12.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "serve": "^14.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "^10.4.19",
    "contentlayer": "^0.3.4",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "@types/mdx": "^2.0.13",
    "jsdom": "^24.1.3",
    "next-contentlayer": "^0.3.4",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.20.6",
    "typescript": "5.4.5",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@10.11.0"
}
